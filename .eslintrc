{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
        "no-unused-vars": [
            2,
            {
                "argsIgnorePattern": "^_"
            }
        ],
        "semi": [
            2,
            "always"
        ],
        "react/prop-types": 0,
        "quotes": [
            2,
            "double"
        ]
    },
    "env": {
        "jest": true,
        "es6": true,
    }
}
